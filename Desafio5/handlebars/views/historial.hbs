<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">Nombre</th>
        <th scope="col">Precio</th>
        <th scope="col">Imagen</th>
    </tr>
    </thead>
    <tbody>
    {{#each products}}
    <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td>
            <img src={{this.thumbnail}} style="height: 40px; width: 40px">
        </td>
    </tr>
    {{/each}}
    </tbody>
</table>